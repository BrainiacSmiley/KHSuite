<%= render 'shared/error_messages', :object => f.object %>
<div class="field">
  <%= f.label :server %><br />
  <%= f.select :server, option_tags = %w[ 1 2 3 4 5 6 7 8 9 ] %>
</div>
<div class="field">
  <%= f.label :name %><br />
  <%= f.text_field :name %>
</div>
<div class="field">
  <%= f.label :avatar %> *<br />
  <%= f.text_field :avatar, :size => 80 %><br />
  <%= t(:only_upjers_pics)%>
</div>
<div class="field">
  <%= f.label :level %><br />
  <%= f.number_field :level %>
</div>
<div class="field">
  <%= f.label :av, "AV" %><br />
  <%= f.text_field :av %>
</div>